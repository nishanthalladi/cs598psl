   "source": [
    "# Take in mean and variance, return probability\n",
    "def mypdf (x, mean, stdev):\n",
    "    return (1 / (stdev * math.sqrt(2 * math.pi))) * math.exp(-0.5 * (((x - mean) / stdev) ** 2))"
   ]
